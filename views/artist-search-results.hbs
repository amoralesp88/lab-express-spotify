<div class="artistDiv">
    {{#each artistsSearch as |artist|}}
    <div class="card" style="width: 18rem;">
        <img src="{{artist.images.[0].url}}" class="card-img-top" alt="{{artist.name}}">
        <div class="card-body">
            <h5 class="card-title">{{artist.name}}</h5>
            <a href="/albums/{{artist.id}}" class="btn btn-primary">View Albums</a>
            {{/each}}
        </div>
    </div>
</div>